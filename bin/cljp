#!/usr/bin/env perl

use strict;
use warnings;
use File::Basename;
use File::Spec;
use lib File::Spec->rel2abs(dirname(__FILE__) . "/../lib");

use CljPerl;

my $cljp = CljPerl::Eval->new();

my $lib_path = File::Spec->rel2abs(dirname(__FILE__) . "/../lib");
$cljp->load("$lib_path/core.clp");
foreach my $f (@ARGV) {
  $cljp->load($f);
}
